{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:android:release": "npm run bundle:android && cd android && ./gradlew assembleRelease",
    "clean": "cd android && ./gradlew clean && cd .. && npx react-native start --reset-cache",
    "reset": "npm run clean && npm install && npx react-native start --reset-cache",
    "fix:red-screen": "powershell -ExecutionPolicy Bypass -File fix-red-screen.ps1",
    "fix:vector-icons": "node fix-vector-icons.js",
    "copy-fonts": "node -e \"const fs=require('fs');const path=require('path');const source='node_modules/react-native-vector-icons/Fonts';const dest='android/app/src/main/assets/fonts';if(!fs.existsSync(dest))fs.mkdirSync(dest,{recursive:true});fs.readdirSync(source).filter(f=>f.endsWith('.ttf')).forEach(f=>fs.copyFileSync(path.join(source,f),path.join(dest,f)));console.log('Fonts copied successfully!');\"",
    "copy-ios-fonts": "node copy-ios-fonts.js",
    "clear-auth": "node clearAuth.js",
    "setup-icons": "npm run copy-fonts && npm run fix:vector-icons && npm run copy-ios-fonts"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.6.0",
    "lottie-react-native": "^7.3.1",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-app-auth": "^8.0.3",
    "react-native-config": "^1.5.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-gifted-charts": "^1.4.63",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.19.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-vision-camera": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/codegen": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.4",
    "@types/react": "^19.0.21",
    "@types/react-navigation": "^3.0.8",
    "babel-jest": "^29.6.4",
    "eslint": "^8.57.0",
    "jest": "^29.6.4",
    "prettier": "^3.2.5",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=18"
  }
}
